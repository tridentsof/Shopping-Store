    <style>
        * {
            margin: 0;
            padding: 0;
        }

        .header {
            position: relative;
            width: 100%;
            background-color: #e77f67;
            height: 50px;
        }

        .logo {
            position: absolute;
            width: 200px;
            height: 100%;
            margin-left: 0px;
            background-color: #f19066;
        }

        .text {
            width: 100%;
            font-weight: 500;
            font-family: Arial, Helvetica, sans-serif;
            font-size: 20px;
            color: white;
            text-align: center;
            padding-top: 15px;
        }

        .notification {
            height: 100%;
            height: 100%;
            position: absolute;
            margin-left: 1200px;
            overflow: hidden;
        }

            .notification ul {
                height: 100%;
                display: flex;
                list-style: none;
            }

                .notification ul li {
                    height: 100%;
                    padding: 13px;
                }

                    .notification ul li a {
                        color: white;
                        text-decoration: none;
                    }

                    .notification ul li:hover {
                        background-color: #f19066;
                    }

        .body-content {
            position: relative;
            width: 100%;
            height: auto;
        }

        .left {
            position: absolute;
            width: 200px;
            height: 1000px;
            background-color: #2f3542;
            margin-left: 0;
        }

        .user {
            position: relative;
            width: 250px;
            height: 70px;
        }

        .avatar {
            position: absolute;
            margin-left: 5px;
            margin-top: 15px;
        }

            .avatar img {
                width: 40px;
                border-radius: 25px;
            }

        .info {
            position: absolute;
            margin-left: 55px;
            margin-top: 20px;
        }

            .info p {
                color: white;
                font-weight: 600;
            }

            .info a {
                text-decoration: none;
                color: white;
                font-size: 10px;
                font-weight: 100;
            }

                .info a i {
                    color: green;
                    margin-right: 2px;
                    font-size: 10px;
                }

        .search-box {
            position: relative;
            margin-top: 10px;
            border: none;
            width: 190px;
            background-color: #95afc0;
            height: 30px;
            margin-left: 5px;
            border-radius: 5px;
        }

        input[type=search]:focus {
            background-color: white;
        }

        .search-text {
            padding-left: 5px;
            position: absolute;
            border: none;
            background-color: #95afc0;
            outline: none;
            height: 30px;
            width: 190px;
            border-radius: 5px;
        }

        .search-icon {
            position: absolute;
            border: none;
            margin-left: 170px;
            margin-top: 7px;
        }

            .search-icon a {
                text-decoration: none;
                color: #2f3542;
            }

        .manager-menu {
            margin-top: 10px;
            width: 200px;
        }

            .manager-menu h4 {
                overflow: hidden;
                padding-left: 10px;
                font-weight: 300;
                width: 190px;
                height: 25px;
                color: #7f8fa6;
                background-color: #353b48;
            }

        .sub {
            width: 200px;
            overflow: hidden;
            list-style: none;
            margin-top: 0px;
        }

            .sub a {
                text-decoration: none;
                color: #f19066;
                width: 200px;
            }

        .btn {
            position: relative;
            display: block;
            height: 30px;
            padding-left: 10px;
            padding-right: 20px;
            padding-top: 20px;
            padding-bottom: 20px;
            width: 200px;
        }

            .btn:hover {
                border-left: 2px solid #f19066;
            }

            .btn p {
                height: 100%;
                position: absolute;
            }

        .drop {
            position: absolute;
            margin-left: 180px;
        }

        .smenu {
            background-color: #95afc0;
            opacity: 0;
            max-height: 0;
            transition: 0.7s;
            width: 200px;
        }

            .smenu a {
                color: white;
                width: 200px;
                display: block;
                padding: 10px;
            }

                .smenu a:hover {
                    border-left: 2px solid #f19066;
                }

        .sub:target .smenu {
            max-height: 10em;
            opacity: 1;
        }

        .right {
            position: absolute;
            margin-left: 200px;
        }
    </style>
